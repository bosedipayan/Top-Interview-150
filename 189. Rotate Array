class Solution {
public:
    void rotate(vector<int>& nums, int k) {
        int n=nums.size();
        vector<int> res(n, 0);
        
        for(int i=0;i<n;i++)
        {
            res[(i+k)%n]=nums[i];
        }
        nums=res;
//         deque<int> dq;
        
//         for(auto it:nums) dq.push_back(it);
        
//         while(k--)
//         {
//             int res=dq.back();
//             dq.pop_back();
            
//             dq.push_front(res);
//         }
        
//         vector<int> res;
//         while(!dq.empty())
//         {
//             res.push_back(dq.front());
//             dq.pop_front();
//         }
//         nums=res;
    }
};
